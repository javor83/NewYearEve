@model IEnumerable<GuestResponse>
@if(Model.Any())
{
<table class="table table-striped table-bordered">

    @foreach (var k in Model)
    {
        <tr>
            <td>@k.Name</td>
            <td>@k.Email</td>
            <td>@k.Phone</td>
            <td>@k.WillAttend</td>
        </tr>    
    }
    </table>
}else
{
    
}

<div>
    <a asp-action="Invite" asp-controller="Home" class="btn btn-primary">@resx_view_Index.Invite</a>
</div>